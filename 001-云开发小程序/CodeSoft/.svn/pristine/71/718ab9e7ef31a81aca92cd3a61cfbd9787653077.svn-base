page{
  background-color: #fff;
}
.star-tip{
  background-color: #f5e941;
  text-align: right;
  padding:2rpx 6rpx;
  margin-top:2rpx;
  color: #2b2b2b;
  font-size: 24rpx;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #191919;
  background-color: #fff;
} 
.grid{
  width: 95%;
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
  padding:10rpx;
  font-size: 28rpx;
  margin-top: 20rpx;
}
.icon{
  width: 64rpx;
  height: 64rpx;
  flex-shrink: 0;
}
.icon-name{
  font-size:24rpx;
}

/**自适应宽度**/
.ux-col-1{
  width: 120rpx;
  height: 120rpx;
  text-align: center;
  padding: 10rpx;
  margin: 15rpx;
  background-color: #efefef;
  border-radius: 20rpx;
}
 
 
 